{
  "version": 2,
  "builds": [
    {
      "src": "foodProject/manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "foodProject/manage.py"
    }
  ],
  "env": {
    "DJANGO_SECRET_KEY": "@django-secret-key",
    "DATABASE_URL": "@database-url",
    "ALLOWED_HOSTS": "your-app-name.vercel.app"
  }
}
